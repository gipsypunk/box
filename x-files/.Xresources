!=============================================================!
!                          UXTerm                             !
!=============================================================!
!FONTS
UXTerm*utf8: 1
UXTerm*locale: true
UXTerm*utf8Fonts: always
UXTerm*renderFont: true
UXTerm*activeIcon: true

UXTerm*faceName: DejaVuSansMono Nerd Font
!UXTerm.vt100.faceNameDoublesize: Hurmit Nerd Font
UXTerm*faceSize:  10





UXTerm*VT100*geometry: 100x30
UXTerm*saveLines: 5000
UXTerm*loginShell: true
UXTerm*charClass: 33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
UXTerm*termName: xterm-color256
UXTerm*eightBitInput: false
UXTerm*bellIsUrgent: false
UXTerm*borderWidth: 0
UXTerm*visualBell:  false
UXTerm*cursorBlink: true
UXTerm*selectToClipboard: true
UXTerm*rightScrollBar: false
UXTerm*scrollBar: false
UXTerm*foreground: #66FF00
UXTerm*background: #282828
UXTerm*cursorColor: #4CBB17
UXTerm*translations: #override \
	Ctrl  <KeyPress> C: copy-selection(PRIMARY, CLIPBOARD, CUT_BUFFER0) \n\
	Ctrl  <KeyPress> V: insert-selection(PRIMARY, CLIPBOARD, CUT_BUFFER0)


UXTerm*highlightSelection: true
!UXTerm*trimSelection: true
!UXTerm*shading:20
